{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-unused-vars": "off",
      "no-param-reassign": ["error", { "props": false }],
      "no-restricted-syntax": "off",
      "guard-for-in": "off"
    }
}
